<div class="log-in-with-oauth">
  <%= link_to user_google_oauth2_omniauth_authorize_path, method: :post, class: "waves-effect waves-dark btn log-in-with-google" do %>
    <i class="fab fa-google"></i> Log in with Google
  <% end %><br>

  <%= link_to user_facebook_omniauth_authorize_path, method: :post, class: "waves-effect waves-dark btn log-in-with-facebook" do %>
    <i class="fab fa-facebook-f"></i>Log in with Facebook
  <% end %>

  <%= link_to user_twitter_omniauth_authorize_path, method: :post, class: "waves-effect waves-dark btn log-in-with-twitter" do %>
    <i class="fab fa-twitter"></i>Log in with Twitter
  <% end %>
</div>